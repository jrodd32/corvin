{
    "name": "doeanderson/super-image",
    "description": "Resizes images after upload",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "super-image"
    ],
    "support": {
        "docs": "https://github.com/doeanderson/super-image/blob/master/README.md",
        "issues": "https://github.com/doeanderson/super-image/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Doe-Anderson",
            "homepage": "https://www.doeanderson.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "doeanderson\\superimage\\": "src/"
        }
    },
    "extra": {
        "name": "Super Image",
        "handle": "super-image",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/doeanderson/super-image/master/CHANGELOG.md",
        "components": {
            "resize": "doeanderson\\superimage\\services\\Resize"
        },
        "class": "doeanderson\\superimage\\SuperImage"
    }
}
