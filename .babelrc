{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "corejs": "3",
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "not dead",
          "not ie 10-11"
        ]
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import"
  ]
}
