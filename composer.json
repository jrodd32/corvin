{
  "require": {
    "craftcms/cms": "^3.0.0",
    "craftcms/feed-me": "^4.1",
    "craftcms/redactor": "2.3.3.2",
    "verbb/super-table": "2.1.20",
    "vlucas/phpdotenv": "^2.4.0",
    "doeanderson/super-image": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "modules\\contactformmodule\\": "doe/contactformmodule/src/",
      "modules\\exceptionreporter\\": "doe/exception-reporter/src/",
      "modules\\superapi\\": "doe/super-api/src",
      "modules\\supersearch\\": "doe/super-search/src",
      "modules\\supervalues\\": "doe/super-values/src"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "doe/super-image"
    }
  ],
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
